<body class="wingmanMain">
	<div class="wingmanText">Wingman</div>
	<div id="ip-container" class="ip-container">
		<!-- initial header -->
		<header class="ip-header">
			<h1 class="ip-logo">
				<i id="icon" class="fa fa-connectdevelop" style="text-shadow: none; font-size: 200px; color: rgb(41, 164, 104); height: 320px; width: 320px; line-height: 150px; border-radius: 0%; text-align: center;"></i>
				</h1>
			<div class="ip-loader">
				<div class="wingmanTextLoading">Initializing Wingman</div>
				<svg class="ip-inner" width="60px" height="60px" viewBox="0 0 80 80">
					<path class="ip-loader-circlebg" d="M40,10C57.351,10,71,23.649,71,40.5S57.351,71,40.5,71 S10,57.351,10,40.5S23.649,10,40.5,10z" />
					<path id="ip-loader-circle" class="ip-loader-circle" d="M40,10C57.351,10,71,23.649,71,40.5S57.351,71,40.5,71 S10,57.351,10,40.5S23.649,10,40.5,10z" />
				</svg>
			</div>
		</header>

		<!-- main content -->
		<div class="ip-main">
			<div id="background"></div>
			<div id="appContainer" class="appContainer jumbotron">
				<div class="main clearfix">
					<div id="nl-form" class="nl-form">
						I have
						<input type="text" name="money" value="" placeholder="$$$" data-subline="For example: <em>$300</em> or <em>$45.50</em>" /> for
						<input type="text" name="people" value="" placeholder="2" data-subline="For example: <em>4</em>" /> people.
						<br> We want
						<select name="food">
							<option value="1" selected>any food</option>
							<option value="2">Indian food</option>
							<option value="3">French food</option>
							<option value="4">Japanese food</option>
							<option value="5">Mexican food</option>
							<option value="6">Chinese food</option>
							<option value="2">Italian food</option>
						</select>
						<select name="time">
							<option value="1" selected>any time</option>
							<option value="3">right now</option>
							<option value="4">at 7 pm</option>
							<option value="5">at 8 pm</option>
							<option value="6">at 9 pm</option>
							<option value="7">at 10 pm</option>
							<option value="2">at 11 pm</option>
						</select>
						around
						<input name="location" type="text" value="" placeholder="here" data-subline="For example: <em>Boston</em> or <em>Los Angeles</em>" />.
						<br> I
						<select name="uber">
							<option value="1" selected>need</option>
							<option value="2">don't need</option>
						</select> an Uber.
						<div class="nl-submit-wrap">
							<button class="nl-submit" id="makeMeHappy">Make me happy</button>
						</div>
						<div class="foodList">
							<p>Pick something to eat:</p>

						</div>
						<div class="movieList">
							<p>Pick something to watch:</p>

						</div>
						<div class="planList">
							<p class="totalCost">Here's your plan! Your total cost for this plan is $100.</p>
							<p class="start glyphicon glyphicon-home" />
							<div class="arrow"><i class="glyphicon glyphicon-arrow-right"></i></div>
							<p class="time time0">20m</p>
							<p class="money money0">$10</p>
							<div class="cardLayout finalFood">
								<div class="top">
									<div class="bkg"></div>
									<div class="name"></div>
									<div class="location"></div>
								</div>
								<div class="bottom">
									<div class="stars"></div>
									<div class="price" id="priceText"></div>
								</div>
							</div>

							<div class="arrow"><i class="glyphicon glyphicon-arrow-right"></i></div>
							<p class="time time1">20m</p>
							<p class="money money1">$10</p>
							<div class="cardLayout finalMovie">
								<div class="top">
									<div class="bkg"></div>
									<div class="name"></div>
									<div class="location"></div>
								</div>
								<div class="bottom">
									<div class="stars"></div>
									<div class="price"></div>
								</div>
							</div>

							<div class="arrow"><i class="glyphicon glyphicon-arrow-right"></i></div>
							<p class="time time2">20m</p>
							<p class="money money2">$10</p>
							<p class="finish glyphicon glyphicon-home" />
						</div>
						<div class="nl-overlay"></div>
					</div>
				</div>

				<!--<div class="footer">&copy; Wingman, 2016</div>-->
			</div>
		</div>
		<!-- /container -->
		<div class="modal">
			<!-- Place at bottom of page -->
		</div>

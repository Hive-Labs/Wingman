		<script src="/js/pathLoader.js"></script>
		<script src="/js/main.js"></script>
		<script src="/js/backendGlue.js"></script>
		<script src="/js/loadingModal.js"></script>
		<script src="/js/recorder.js"></script>
</body>

</html>
